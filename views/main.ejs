<%- include('./header.ejs') %>
<div class="mainWindow">
<% data.forEach(ds => { %>
    <div class="line">
    <% ds.forEach(d => { %>
        <div class="product" onclick="location.href += 'product/<%= d.id %>'">
            <img src="<%= d.titleImg %>" alt="no Image">
            <h4><%= d.name %></h4>
        </div>
    <% }) %>
    </div>
<% }) %>
</div>
<%- include('./footer.ejs') %>